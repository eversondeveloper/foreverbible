{
  "expo": {
    "name": "foreverbible",
    "slug": "foreverbible",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B1B1B",
      "plugins": [
      [
        "expo-font",
        {
          "custom": {
            "FontAwesome": "node_modules/react-native-vector-icons/Fonts/FontAwesome.ttf"
          }
        }
      ]
    ],
    "extra": {
      "fontAwesome": "FontAwesome"
    }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true
    },
    "android": {
      "versionCode": 4, 
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.evesonsilvadeveloper.foreverbible",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d1e3e91f-2843-484c-973c-9c72169fa720"
      }
    },
    "owner": "evesonsilvadeveloper"
  }
}
